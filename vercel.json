{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "api/index.ts": {
      "includeFiles": [
        "src/docs/**/*",
        "node_modules/swagger-ui-dist/**/**"
      ]
    }
  },
  "routes": [
    { "src": "/(.*)", "dest": "api/index.ts" }
  ]
}
